package project;

public class Score {

	public static void main(String[] args) {
        int student=35;
		
		if(student >=60) {
			System.out.println("first class");
		}
		else if (student>=45 && student <60){
			System.out.println("second class");
		}
		else {
			System.out.println("third class");
		}

	}

}
